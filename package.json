{
  "name": "@zubenelakrab/gitstats",
  "version": "0.1.0",
  "description": "Powerful Git repository analyzer with comprehensive statistics and insights",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "gitstats": "./src/cli/index.ts"
  },
  "scripts": {
    "start": "node --experimental-strip-types src/cli/index.ts",
