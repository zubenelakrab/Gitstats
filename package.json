  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zubenelakrab/Gitstats.git"
  },
  "homepage": "https://github.com/zubenelakrab/Gitstats#readme",
  "bugs": {
    "url": "https://github.com/zubenelakrab/Gitstats/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cli-table3": "^0.6.5",
    "ora": "^8.1.0",
    "date-fns": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
